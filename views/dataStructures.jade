script(src="js/dataStructures/queue.js")
script(src="js/dataStructures/linkedList.js")

script
    var currentAlgorithmFile = "!{currentAlgorithm.file}";
    $(function() {
        var ds;
        if (currentAlgorithmFile == "stack.js")
            ds = new Stack();
        else if (currentAlgorithmFile == "queue.js")
            ds = new Queue();
        else if (currentAlgorithmFile == "linkedList.js")
            ds = new LinkedList();
        dataStructures.init(ds);
    });

